                        <li><%= (link_to "Contact", contact_path, :id => "contact", :class => controller_name === "contact" ? "active" : "") %></li>
                    </ul>
                    <div class="glow"></div>
                </nav>
                <section class="logo"></section>
            </header>

            <% else %>

